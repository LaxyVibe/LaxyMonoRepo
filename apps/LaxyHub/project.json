{
  "name": "laxy-hub",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/LaxyHub/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "command": "react-scripts build",
        "cwd": "apps/LaxyHub",
        "outputPath": "apps/LaxyHub/build"
      },
      "configurations": {
        "development": {
          "command": "react-scripts build",
          "cwd": "apps/LaxyHub"
        },
        "production": {
          "command": "react-scripts build",
          "cwd": "apps/LaxyHub"
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "BROWSER=none react-scripts start",
        "cwd": "apps/LaxyHub"
      },
      "configurations": {
        "development": {
          "command": "BROWSER=none PORT=4200 react-scripts start",
          "cwd": "apps/LaxyHub"
        },
        "production": {
          "command": "BROWSER=none PORT=4200 react-scripts start",
          "cwd": "apps/LaxyHub"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/LaxyHub/jest.config.ts"
      }
    },
    "fetch-api": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/fetch-api-data.js",
        "cwd": "apps/LaxyHub"
      }
    }
  },
  "tags": ["type:app", "scope:laxyhub"]
}
