{
  "name": "laxy-studio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/LaxyStudio/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "command": "vite build",
        "cwd": "apps/LaxyStudio",
        "outputPath": "apps/LaxyStudio/build"
      },
      "configurations": {
        "development": {
          "command": "vite build",
          "cwd": "apps/LaxyStudio"
        },
        "production": {
          "command": "vite build",
          "cwd": "apps/LaxyStudio"
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "vite",
        "cwd": "apps/LaxyStudio"
      },
      "configurations": {
        "development": {
          "command": "vite --port 4202",
          "cwd": "apps/LaxyStudio"
        },
        "production": {
          "command": "vite --port 4202",
          "cwd": "apps/LaxyStudio"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/LaxyStudio/jest.config.ts"
      }
    }
  },
  "tags": []
}
